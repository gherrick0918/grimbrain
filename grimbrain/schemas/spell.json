{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Spell",
  "type": "object",
  "required": [
    "name",
    "level",
    "school",
    "casting_time",
    "range",
    "components",
    "duration",
    "classes",
    "text",
    "provenance"
  ],
  "properties": {
    "name": {"type": "string"},
    "level": {"type": "integer"},
    "school": {"type": "string"},
    "casting_time": {"type": "string"},
    "range": {"type": "string"},
    "components": {"type": "string"},
    "duration": {"type": "string"},
    "classes": {"type": "array", "items": {"type": "string"}},
    "text": {"type": "string"},
    "provenance": {"type": "array"}
  }
}
